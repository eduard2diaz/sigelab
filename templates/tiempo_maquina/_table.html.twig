<table class="table table-hover" id="tiempo_maquinatable">
    <thead>
    <tr>
        <th>{{ 'row_number' | trans }}</th>
        <th>{{ 'user' |trans }}</th>
        <th>{{ 'pc' |trans }}</th>
        <th>{{ 'startdate_field' |trans }}</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    {%set i=1%}
    {% for tiempo_maquina in tiempo_maquinas %}
        <tr>
            <td>{{ i }}</td>
            <td>{{ tiempo_maquina.usuario }}</td>
            <td>{{ tiempo_maquina.pc.numero~'/'~tiempo_maquina.laboratorio.nombre }}</td>
            <td>{{ tiempo_maquina.fechaInicio ? tiempo_maquina.fechaInicio|date('Y-m-d H:i:s') : '' }}</td>
            <td>
                <ul class="nav nav-pills" >
                    <li class="nav-item m-tabs__item">
                        <a class="btn btn-sm btn-default text-uppercase machine_time_show" data-href="{{ path('tiempo_maquina_show', { 'id': tiempo_maquina.id }) }}">
                            <i class="flaticon-eye"></i>{{ 'visualize' |trans }}
                        </a>
                    </li>
                    <li class="nav-item m-tabs__item">
                        <a class="btn btn-sm btn-info text-uppercase edicion" data-href="{{ path('tiempo_maquina_edit', { 'id': tiempo_maquina.id }) }}">
                            <i class="flaticon-edit-1"></i>{{ 'edit' |trans }}
                        </a>
                    </li>
                    <li class="nav-item m-tabs__item">
                        <a class="btn btn-sm btn-danger text-uppercase eliminar_tiempo_maquina" data-href="{{ path('tiempo_maquina_delete', { 'id': tiempo_maquina.id }) }}">
                            <i class="flaticon-delete-1"></i>{{ 'delete' |trans }}
                        </a>
                    </li>
                </ul>
            </td>
        </tr>
        {%set i=1+i%}
    {% endfor %}
    </tbody>
    <tfoot>
    <tr>
        <th>{{ 'row_number' | trans }}</th>
        <th>{{ 'user' |trans }}</th>
        <th>{{ 'pc' |trans }}</th>
        <th>{{ 'startdate_field' |trans }}</th>
        <th></th>
    </tr>
    </tfoot>
</table>
